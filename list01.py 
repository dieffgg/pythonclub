def recebeLista():
    lista = []
    j=0
    a = 0
    while(len(lista) < 3):
        lista.append(int(input('digite...')))
        a += lista[j]
        j += 1
    print(lista)
    print(type(lista))
    print(a)
    somaLista(lista)


def somaLista(*args):
    i=0
    soma=0
    while i < len(args):
        print(type(args[i]))
        soma += args
        print(soma)
        i += 1

recebeLista()

